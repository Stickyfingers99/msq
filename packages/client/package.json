{
  "name": "@fort-major/msq-client",
  "version": "0.2.8",
  "main": "./dist/cjs/index",
  "module": "./dist/esm/index",
  "unpkg": "./dist/esm/index",
  "types": "./dist/esm/index.d.ts",
  "license": "MIT",
  "scripts": {
    "prod:build": "rm -rf ./dist && tsc && tsc -p tsconfig.cjs.json && node inline-env-vars.js",

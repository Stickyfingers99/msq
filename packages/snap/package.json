{
  "name": "@fort-major/masquerade",
  "version": "0.2.2",
  "description": "Privacy-focused MetaMask snap for the Internet Computer (ICP)",
  "keywords": [
    "MetaMask",
    "web3",
    "Internet Computer",
    "ICP",
    "wallet",
    "authorization",
    "login",
    "crypto",
    "icrc1",
    "agent"
  ],
  "main": "dist/bundle.js",
  "types": "",
  "author": "seniorjoinu <senior.joinu@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/fort-major/masquerade.git"
  },
  "bugs": {
    "url": "https://github.com/fort-major/masquerade/issues"
  },
  "homepage": "https://github.com/fort-major/masquerade#readme",
  "scripts": {
    "prod:build": "tsc && pnpm run version:set && mm-snap build -s src/index.ts && mm-snap manifest --fix",
    "prod:pub": "npm publish",
    "dev:build": "tsc && mm-snap build -s src/index.ts",
    "dev": "mm-snap watch -s src/index.ts -d dist",
    "dev:runSnap": "pnpm run dev",
    "test": "jest",
    "lint": "eslint --fix src --ext .ts --ext .tsx",
    "format": "pnpm exec prettier src/**/*.{ts,tsx} --write",
    "version:set": "node ../../set-version.js",
    "doc": "typedoc src/**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@metamask/snaps-cli": "^1.0.2",
    "@metamask/snaps-jest": "^1.0.0",
    "@metamask/snaps-types": "^1.0.2",
    "@types/jest": "29.5.4",
    "@types/lodash.debounce": "^4.0.8",
    "@types/node": "20.7.1",
    "cbor-x": "1.5.4",
    "envify": "4.1.0",
    "jest": "29.6.4",
    "ts-jest": "29.1.1"
  },
  "dependencies": {
    "@dfinity/identity-secp256k1": "0.19.3",
    "@fort-major/masquerade-shared": "workspace:*",
    "@metamask/snaps-ui": "1.0.2",
    "lodash.debounce": "^4.0.8"
  }
}
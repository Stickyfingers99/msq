{
    "$schema": "https://turbo.build/schema.json",
    "globalDotEnv": [
        ".env.dev",
        ".env.prod"
    ],
    "pipeline": {
        "prod:build": {
            "dependsOn": [
                "^prod:build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "prod:pub": {
            "dependsOn": [
                "^prod:pub"
            ]
        },
        "dev:build": {
            "dependsOn": [
                "^dev:build"
            ],
            "outputs": [
                "dist/**"
            ]
        },
        "dev": {
            "cache": false
        },
        "dev:deploy": {
            "cache": false
        },
        "dev:nns": {
            "cache": false
        },
        "dev:gen": {
            "cache": false
        },
        "dev:start": {
            "cache": false
        },
        "test": {
            "cache": false
        },
        "version:set": {
            "cache": false
        },
        "cargo:repair": {
            "cache": false
        },
        "lint": {}
    }
}
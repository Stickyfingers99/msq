{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "dist",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowJs": true,
    "declaration": true,
    "declarationDir": "dist",
    "emitDeclarationOnly": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "paths": {
      "#assets/*": ["./assets/*"]
    }
  },
  "types": ["vite/client"],
  "references": [
    {
      "path": "../../packages/client"
    }
  ],
  "include": ["./src"]
}
